<?xml version="1.0" ?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Config:etc/system_file.xsd">
	<system>
		<section id="linkaccount" showInDefault="1" showInStore="1" showInWebsite="1" sortOrder="10" translate="label">
			<label>AR Settings</label>
			<tab>curacao_settings</tab>
			<resource>Dyode_ARWebservice::config_dyode_arwebservice</resource>
			<group id="curacao" showInDefault="1" showInStore="1" showInWebsite="1" sortOrder="10" translate="label">
				<label>Settings</label>
				<field id="apiurl" showInDefault="1" showInStore="1" showInWebsite="1" sortOrder="10" translate="label" type="text">
					<label>API Url</label>
					<comment/>
				</field>
				<field id="apikey" showInDefault="1" showInStore="1" showInWebsite="1" sortOrder="10" translate="label" type="text">
					<label>API Key</label>
					<comment/>
				</field>
				<field id="apiuser" showInDefault="1" showInStore="1" showInWebsite="1" sortOrder="10" translate="label" type="text">
					<label>Username</label>
					<comment/>
				</field>
				<field id="apipass" showInDefault="1" showInStore="1" showInWebsite="1" sortOrder="10" translate="label" type="text">
					<label>Password</label>
					<comment/>
				</field>
				<field id="phonewsdlurl" showInDefault="1" showInStore="1" showInWebsite="1" sortOrder="10" translate="label" type="text">
					<label>Phone WSDL Url</label>
					<comment/>
				</field>
				<field id="backupurl" showInDefault="1" showInStore="1" showInWebsite="1" sortOrder="10" translate="label" type="text">
					<label>Phone Backup URL</label>
					<comment/>
				</field>
				<field id="licensekey" showInDefault="1" showInStore="1" showInWebsite="1" sortOrder="10" translate="label" type="text">
					<label>License Key</label>
					<comment/>
				</field>
				<field id="callerid" showInDefault="1" showInStore="1" showInWebsite="1" sortOrder="10" translate="label" type="text">
					<label>Caller Id</label>
					<comment/>
				</field>
			</group>
		</section>
	</system>
</config>
