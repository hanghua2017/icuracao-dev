<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <aw_store_locator>
            <general>
                <enable_store_locator>1</enable_store_locator>
                <title>Store Locations</title>
                <url_key>google-map</url_key>
                <google_maps_api_key></google_maps_api_key>
                <header_block></header_block>
                <enable_find_my_location_button>0</enable_find_my_location_button>
            </general>
            <search>
                <search_radius_values>1,3,5,10,15,25,50,100,500</search_radius_values>
                <default_search_radius>100</default_search_radius>
                <default_search_measurement>km</default_search_measurement>
            </search>
         </aw_store_locator>
    </default>
</config>
